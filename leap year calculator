year = int(input("Enter your year :"))
if year % 4 == 0:
    if year % 100 != 0:
        print(f"Entered year is {year} is leap year")
    else:
            if year % 400 == 0:
                print(f"Entered year {year} is leap year")
            else:
                print(f"Entered year is {year} is not leap year")              
else:
    print(f"Entered year is {year} is not leap year")
    
